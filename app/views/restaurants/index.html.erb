<div class="container">
	<ul class="list-group list-group-flush">
		<% @restaurants.each do |restaurant| %>
			<li class="list-group-item">
				<%= restaurant.name %>
				<%= link_to 'See details', restaurant_path(restaurant)  %>
			</li>
		<% end %>
		<%= link_to 'Add a new resturant', new_restaurant_path %>
	</ul>
</div>

